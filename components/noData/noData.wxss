.noData{
  margin-bottom: 30rpx;
}
.qz-noda-box {
  text-align: center;
  padding-top: 100rpx;
}

.qz-noda-box .a-image {
  width: 242rpx;
  height: 160rpx;
  background-size: cover;
}

.a-title {
  font-size: 24rpx;
  font-weight: 400;
  color: #999999;
  margin-top: 32rpx;
}

.foot-divider {
  height: 1px;
  background-color: #dcdada;
  width: 63rpx;
  margin: 0 22rpx;
}

.foot-text {
  font-size: 26rpx;
  color: #a1a1a1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.foot-layout {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 115rpx;
}

.loading {
  display: inline-block;
  margin-right: 12px;
  vertical-align: middle;
  width: 14px;
  height: 14px;
  background: 0 0;
  border-radius: 50%;
  border: 2px solid #e9eaec;
  border-color: #e9eaec #e9eaec #e9eaec #f4a738;
  animation: btn-spin 0.6s linear;
  animation-iteration-count: infinite;
}

@keyframes btn-spin {
  0% {
    transform: rotate(0);
  }

  100% {
    transform: rotate(360deg);
  }
}
