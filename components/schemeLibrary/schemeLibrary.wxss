@import "/components/noData/noData.wxss";
.schemeList {
  padding-top: 44rpx;
}
.schemeOne {
  background: #ffffff;
  border-radius: 4rpx;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 13, 35, 0.07);
  padding: 32rpx 24rpx 0;
  margin-bottom: 26rpx;
}
.schemeOne:last-child {
  margin-bottom: 0;
}
.schemeOne>.schemeName {
  color:var(--colorOne);
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.schemeOne>.schemeDate {
  color: #bdc0c4;
  font-size: 22rpx;
  margin-bottom: 30rpx;
}
.schemeOne>.schemeDesc {
  font-size: 20rpx;
  color: #a6abb5;
  border-top:2rpx solid #f8f8f8;
  height: 78rpx;
}
.schemeOne>.schemeDesc>image {
  width: 12rpx;
  height: 20rpx;
}