/* pages/secondPage/secondPage.wxss */
@import "/pages/index/index.wxss";
@import "/components/noData/noData.wxss";

.van-tabs {
  min-height: 100%;
  height: 100%;
}

.van-tabs__content {
  min-height: 100%;
  height: 100%;
  overflow: auto !important;
}

.van-tab__pane {
  height: 100% !important;
  min-height: 100%;
}

.van-tabs--line .van-tabs__wrap {
  position: fixed;
  width: 100%;
  height: 100rpx !important;
  background: #ffffff;
  box-shadow: 0rpx 12rpx 12rpx 0rpx rgba(0, 13, 35, 0.04);
}

.van-tab {
  line-height: 100rpx !important;
  font-size: 26rpx !important;
  color: #a6abb5 !important;
}

.van-tab::after {
  content: '';
  width: 2rpx;
  height: 50rpx;
  background: rgba(0, 13, 35, .04);
  border-radius: 2rpx;
  position: absolute;
  right: 0rpx;
  top: 26rpx;
}

.van-tabs__line {
  width: 108rpx !important;
  height: 8rpx !important;
  background: #0077cb !important;
  z-index: 0 !important;
  bottom: 36rpx !important;
  left: -16rpx !important;
  border-radius: 4rpx !important;
}

.van-tab--active {
  color: #333333 !important;
  font-size: 28rpx !important;
  font-weight: bold !important;
}

.detailContent {
  padding: 136rpx 30rpx 36rpx;
}

.mainContent {
  background: #ffffff;
  border-radius: 4rpx;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 11, 29, 0.07);
  padding: 20rpx 20rpx 26rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: var(--colorOne);
  margin-top: 44rpx;
  margin-bottom: 60rpx;
}

.mainContent>image {
  height: 366rpx;
  margin-bottom: 22rpx;
}

.mainContent>.mainDetail {
  font-size: 26rpx;
  color: #6f7580;
  line-height: 38rpx;
  letter-spacing: 2rpx;
  font-weight: normal;
  margin-top: 24rpx;
}

.mliepostContent {
  margin: 56rpx 0 20rpx;
}

.mliepostOne {
  margin-left: 72rpx;
  padding: 50rpx 0 54rpx 40rpx;
  border-left: 2rpx dashed rgba(151, 151, 151, .2);
  position: relative;
}

.mliepostOne:first-child {
  padding-top: 0;
}

.mliepostOneYear {
  position: absolute;
  left: -90rpx;
  bottom: -20rpx;
  font-size: 32rpx;
  font-family: DINCondensed, DINCondensed-Bold;
  font-weight: bold;
  color: var(--colorOne);
}

