.mliepostOneHonor {
  font-size: 24rpx;
  color: var(--colorOne);
  /* position: relative; */
  margin-bottom: 36rpx;
}

.mliepostOneHonor:last-child {
  margin-bottom: 0;
}

.mliepostOneHonor>.honorRight {
  position: relative;
  /* height: 144rpx; */
  background: #fff;
  border-radius: 8rpx;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 11, 29, 0.07);
  padding: 24rpx 0 26rpx 40rpx;
  line-height: 34rpx;
}

.mliepostOneHonor>.honorRight::before {
  content: '';
  position: absolute;
  left: -22rpx;
  top: 50%;
  transform: translateY(-50%) rotate(-90deg);
  width: 0;
  height: 0;
  border-width: 0 16rpx 16rpx;
  border-style: solid;
  border-color: transparent transparent #fff;
  /* box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 11, 29, 0.07); */
}

.mliepostOneHonor>.honorRight>.honorDesc {
  position: relative;
}

.mliepostOneHonor>.honorRight>.honorDesc::before {
  content: '';
  position: absolute;
  left: -20rpx;
  top: 14rpx;
  width: 4rpx;
  height: 4rpx;
  background: black;
  border-radius: 50%;
}

.honorMonth {
  position: absolute;
  left: -110rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 22rpx;
  font-weight: bold;
}

.honorMonth::after {
  content: '';
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 62rpx;
  width: 12rpx;
  height: 12rpx;
  background: #eb3e00;
  border-radius: 50%;
}

.honorYear {
  position: absolute;
  bottom: -70rpx;
  left: -124rpx;
  font-size: 24rpx;
  font-weight: bold;
}

.honorYear::after {
  content: '';
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 70rpx;
  width: 20rpx;
  height: 20rpx;
  background: #ffffff;
  border: 4rpx solid #333333;
  border-radius: 50%;
}

.cultureContent {
  margin-top: 42rpx;
}

.cultureOne {
  height: 200rpx;
  background: linear-gradient(211deg, rgba(192, 226, 250, .16) 11%, rgba(217, 235, 254, 0.00) 95%);
  padding: 40rpx 24rpx 0 22rpx;
  color: var(--colorOne);
}

.cultureLogo {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  font-size: 28rpx;
  font-weight: bold;
}

.cultureLogo>image {
  height: 100%;
}

.cultureLogo>text {
  position: absolute;
  right: -98rpx;
  top: 50%;
  transform: translateY(-50%);
}

.cultureText {
  width: 408rpx;
  font-size: 26rpx;
  color: #6f7580;
  line-height: 38rpx;
}

.cultureTwo {
  position: relative;
  height: 318rpx;
  background: linear-gradient(45deg, rgba(187, 196, 236, .2) 2%, rgba(217, 235, 254, 0.00) 68%);
  margin-top: 42rpx;
  padding: 50rpx 22rpx 0 24rpx;
  font-size: 24rpx;
}

.cultureLogo2 {
  position: absolute;
  top: 50rpx;
  right: 22rpx;
}

.cultureLogo2 text,
.cultureLogo3 text {
  left: -98rpx;
  right: 0;
}

.cultureName {
  margin-bottom: 30rpx;
}

.cultureName>text {
  color: #6F7580;
}

.cultureThree {
  padding-right: 64rpx;
  margin-top: 30rpx;
}

.cultureThree .cultureText {
  padding-top: 32rpx;
  text-align: right;
}

.cultureFour {
  height: 200rpx;
  padding-top: 50rpx;
  margin-top: 30rpx;
}

.cultureFour>view:first-child {
  font-size: 26rpx;
  color: #6f7580;
  line-height: 38rpx;
  width: 396rpx;
  padding-top: 12rpx;
}

.cultureFive {
  margin-top: 70rpx;
}

.cultureFive .cultureLogo {
  margin-left: 22rpx;
  margin-bottom: 34rpx;
}

.culturePic>image {
  vertical-align: bottom;
  margin-bottom: 4rpx;
}

.culturePic>image:first-child {
  width: 456rpx;
  height: 330rpx;
  margin-right: 4rpx;
  border-radius: 4rpx 0 0 0;
}

.culturePic>image:nth-child(2) {
  width: 230rpx;
  height: 330rpx;
  border-radius: 0 4rpx 0 0;
}

.culturePic>image:nth-child(3) {
  width: 246rpx;
  height: 248rpx;
  margin-right: 4rpx;
}

.culturePic>image:nth-child(4) {
  width: 440rpx;
  height: 248rpx;
}

.culturePic>image:nth-child(5) {
  width: 344rpx;
  height: 344rpx;
  margin-right: 4rpx;
  border-radius: 0 0 0 4rpx;
}

.culturePic>image:nth-child(6) {
  width: 342rpx;
  height: 344rpx;
  border-radius: 0 0 4rpx 0;
}

.experts,
.qualification {
  padding: 136rpx 0rpx 36rpx;
  height: 100%;
  min-height: 100%;
}

.qualification .allHead {
  margin: 0 30rpx;
}

.qualificationHead {
  height: 100rpx;
  background: #ffffff;
  box-shadow: 0rpx 12rpx 12rpx 0rpx rgba(0, 13, 35, 0.04);
  padding: 0 36rpx;
  font-size: 26rpx;
  color: #a6abb5;
  margin-top: 28rpx;
}

.qualificationHead>view {
  position: relative;
}

.qualificationHead>view:nth-child(-n+2)::after {
  content: "";
  position: absolute;
  right: -88rpx;
  top: -2rpx;
  width: 2rpx;
  height: 50rpx;
  background: rgba(0, 13, 35, .04);
  border-radius: 2rpx;
}

.qualificationActive {
  color: #000d23;
  font-size: 28rpx;
  position: relative;
  font-weight: bold;
  z-index: 0;
}

.qualificationActive::before {
  content: '';
  position: absolute;
  z-index: -1;
  bottom: 4rpx;
  left: 2rpx;
  width: 108rpx;
  height: 8rpx;
  background: #0077cb;
}

.qualificationList {
  padding: 32rpx 30rpx 0;
}

.qualificationImg {
  width: 328rpx;
  height: 438rpx;
  background: rgba(251, 251, 251, .8);
  padding: 18rpx;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
}

.qualificationImg:nth-child(2n) {
  margin-right: 0rpx;
}

.qualificationImg:nth-last-child(-n+2) {
  margin-bottom: 0rpx;
}

.qualificationImg>image {
  height: 100%;
}


.experts .allHead {
  margin: 0 30rpx 30rpx;
}

.expertsOne {
  height: 256rpx;
  position: relative;
  padding: 42rpx 48rpx 0;
  z-index: 0;
}

.expertsOne>.expertsBg {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  z-index: -1;
}

.expertsLeft>image {
  width: 120rpx;
  height: 160rpx;
  margin-right: 24rpx;
}

.expertsName {
  font-size: 32rpx;
  color: var(--colorOne);
  font-weight: bold;
}

.expertsName>.expertsMore {
  color: #a6abb5;
  font-size: 20rpx;
}

.expertsName>.expertsMore>image {
  width: 12rpx;
  height: 20rpx;
  margin-left: 8rpx;
}

.expertsWork {
  font-size: 24rpx;
  color: #a6abb5;
  margin-top: 8rpx;
}

.scrollView {
  flex: 1;
  overflow: hidden;
}