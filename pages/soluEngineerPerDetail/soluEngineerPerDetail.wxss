/* pages/soluEngineerPerDetail/soluEngineerPerDetail.wxss */
@import "../../wxParse/wxParse.wxss";

.program {
  padding: 0 30rpx 100rpx;
  position: relative;
}
.programNo {
  padding-bottom: 30rpx;
}
.programIphoneX {
  padding-bottom: 168rpx;
}

.programLogo>image {
  width: 150rpx;
  height: 112rpx;
}

.programTitle {
  /* height: 112rpx; */
  font-size: 34rpx;
  color: var(--colorOne);
  font-weight: bold;
  margin: 10rpx 0 30rpx;
  text-align: center;
}

.downloadFile {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  /* height: 98rpx; */
  padding: 6rpx 30rpx;
  font-size: 34rpx;
  background: rgba(255, 255, 255, 1);
  font-weight: bold;
}

.downloadFile>button {
  width: 100%;
  height: 86rpx;
  background: #0077cb;
  color: #ffffff;
  font-weight: normal;
  border-radius: 8rpx;
}

.downloadFileOK {
  background: #bdc0c4;
  color: #ffffff;
}

.downFile {
  padding: 28rpx 30rpx 0;
  font-size: 32rpx;
  font-weight: bold;
  color: var(--colorOne)
}

.downloadList {
  margin-bottom: 34rpx;
}

.chooseFile {
  margin-bottom: 36rpx;
}

.downloadFileOne {
  height: 92rpx;
  background: #fafafa;
  font-size: 28rpx;
  color: #6a7484;
  padding-left: 22rpx;
  margin-bottom: 16rpx;
}

.downloadFileOne:last-child {
  margin-bottom: 0rpx;
}

.downloadFileOne>image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 12rpx;
}

.downloadFileOneActive {
  color: var(--colorOne)
}

.downBtn {
  height: 86rpx;
  background: #0077cb;
  border-radius: 8rpx;
  font-size: 34rpx;
  color: #ffffff;
  font-weight: bold;
}

.expolre {
  font-size: 26rpx;
  margin: 8rpx 0 36rpx;
  color: #a6abb5;
}

.applyFormOne {
  height: 92rpx;
  background: #fafafa;
  font-size: 30rpx;
  color: var(--colorOne);
  padding: 0 24rpx;
  font-weight: bold;
  margin-bottom: 22rpx;
}

.applyFormOnePlaceholder {
  color: #a6abb5;
  font-weight: normal;
}

.applyFormOne:nth-child(5) {
  margin-bottom: 36rpx;
}

.applyFormOnlyOne {
  margin: 36rpx 0;
}

.applyFormWarn {
  background: #fff3f0;
  color: #f86038 !important;
}

.applyFormWarnSize {
  color: #f86038 !important;
}

.applyFormKey {
  width: 122rpx;
  font-size: 28rpx;
  color: #a6abb5;
  font-weight: normal;
  margin-right: 26rpx;
}
.okShow {
  height: 200rpx;
  color: var(--colorOne);
  font-size: 30rpx;
}
.iphoneXBottom {
  padding-bottom: 68rpx;
}