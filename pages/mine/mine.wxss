/* pages/mine/mine.wxss */
@import "/components/schemeLibrary/schemeLibrary.wxss";
@import "/components/noData/noData.wxss";
.mineHead {
  position: relative;
}

.mineHead>image {
  height: 404rpx;
}

.mineHead>.mineDetail {
  position: absolute;
  bottom: 0;
  left: 30rpx;
  width: 690rpx;
  height: 188rpx;
  background: #ffffff;
  border-radius: 8rpx;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 0, 0, 0.07);
  padding: 60rpx 0 0 20rpx;
  color: var(--colorOne);
  font-size: 36rpx;
  font-weight: bold;
}

.mineHead>.mineDetail>image {
  width: 68rpx;
  height: 68rpx;
  margin-right: 12rpx;
  margin-top: 12rpx;
}

.mineHead>.mineDetail>.company>view:last-child {
  font-size: 22rpx;
  font-weight: Normal;
  margin-top: 10rpx;
}

.loginOut {
  margin: 40rpx 30rpx 56rpx;
  height: 86rpx;
  background: #f3f4f6;
  border-radius: 8rpx;
  font-size: 34rpx;
  color: #6a7484;
  text-align: center;
  font-weight: bold;
  line-height: 86rpx;
}

.companyAll {
  padding: 0 30rpx 30rpx;
}

.companyList {
  margin-top: 52rpx;
}

.companyTitle {
  font-size: 30rpx;
  color: var(--colorOne);
  font-weight: bold;
  padding-left: 14rpx;
  position: relative;
  height: 88rpx;
}

.companyTitle::before {
  content: '';
  position: absolute;
  left: 0;
  top: 8rpx;
  width: 6rpx;
  height: 72rpx;
  background: #0077cb;
}

.companyName {
  font-size: 26rpx;
  font-weight: Normal;
  margin-top: 8rpx;
}

.companyOne {
  height: 80rpx;
  color: var(--colorOne);
  font-size: 30rpx;
  font-weight: bold;
  border-bottom: 2rpx solid #f8f8f8;
  padding-bottom: 36rpx;
  margin-bottom: 36rpx;
}

.companyOne:last-child {
  margin-bottom: 0;
}

.companyOne>.companyNum {
  font-family: DINAlternate, DINAlternate-Bold;
  margin-right: 8rpx;
}

.companyOne>image {
  width: 12rpx;
  height: 20rpx;
}
