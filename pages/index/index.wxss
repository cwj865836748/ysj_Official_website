/* pages/index/index.wxss */
@import '/components/newsCenter/newsCenter.wxss';
.all {
  padding-bottom: 30rpx;
}
.indexLogo {
  width: 120rpx;
  height: 50rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}

.home_swiper {
  height: 562rpx;
  position: relative;
}

.home_swiper swiper,
image {
  height: 100%;
}

.dots {
  position: absolute;
  bottom: 52rpx;
  left: 0;
  right: 0;
}

.dots .dot {
  margin-left: 12rpx;
  width: 12rpx;
  height: 12rpx;
  background: rgba(0, 0, 0, 0.23);
  border-radius: 50%;
  transition: all .6s;
}

.dots .dot.active {
  width: 32rpx;
  background: #478cfa;
  border-radius: 6rpx;
}

.home_5Sort {
  width: 690rpx;
  height: 138rpx;
  background: #ffffff;
  border-radius: 4rpx;
  box-shadow: 0rpx 2rpx 22rpx 0rpx rgba(0, 11, 29, 0.07);
  margin-left: 30rpx;
  margin-top: -28rpx;
  position: relative;
  padding: 0 20rpx;
  color: var(--colorOne);
}

.sortOne>.sortHead {
  font-size: 26rpx;
  font-family: DINAlternate, DINAlternate-Bold;
  font-weight: bold;
  margin-bottom: 2rpx;
}

.sortOne>.sortHead>image {
  width: 40rpx;
  height: 40rpx;
  margin-left: 16;
}

.sortOne>.sortTitle {
  font-size: 16rpx;
}

.home_6Menu {
  margin-top: 40rpx;
  margin-left: 30rpx;
  font-size: 26rpx;
  color: var(--colorOne);
}

.menuOne {
  width: 216rpx;
  height: 68rpx;
  background: #f6f7f8;
  border-radius: 4rpx;
  margin-right: 20rpx;
}

.menuOne:first-child {
  margin-bottom: 20rpx;
}

.menuOne:nth-child(3n) {
  margin-right: 0rpx;
}

.menuOne>image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 8rpx;
}

.mainBusiness {
  margin: 62rpx 30rpx 0 30rpx;
  position: relative;
}

.allHead {
  color: var(--colorOne);
  position: relative;
  padding-left: 16rpx;
  height: 92rpx;
}

.allHead::before {
  content: '';
  position: absolute;
  left: 0;
  top: 16rpx;
  width: 6rpx;
  height: 72rpx;
  background: #0077cb;
}

.allHead>.title>view {
  font-size: 32rpx;
  margin-bottom: 2rpx;
  font-weight: bold;
}

.allHead>.title>text {
  font-size: 30rpx;
}

.allHead>.toMore {
  color: #a6abb5;
  font-size: 22rpx;
  height: 100%;
}

.allHead>.toMore>image {
  width: 12rpx;
  height: 20rpx;
  margin-left: 8rpx;
}

.mainBusinessContent {
  color: var(--colorOne);
  background: #ffffff;
  border-radius: 4rpx;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 11, 29, 0.07);
  padding: 20rpx 20rpx 0;
  margin-top: 44rpx;
}

.mainBusinessContent>image,.mainBusinessContent>video {
  height: 366rpx;
  margin-bottom: 22rpx;
  border-radius: 8rpx;
}

.mainBusinessContent>.mainBusinessTitle {
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 24rpx;
}

.mainBusinessContent>.mainBusinessDetail {
  font-size: 26rpx;
  color: #6f7580;
  line-height: 38rpx;
  letter-spacing: 2rpx;
}

.mainBusinessContent>.mainBusinessImage {
  height: 504rpx;
  position: relative;
}

.mainBusinessContent>.mainBusinessImage>.mainCenter {
  margin-top: 96rpx;
  width: 326rpx;
  height: 302rpx;
  position: relative;
}

.mainBusinessContent>.mainBusinessImage>.mainCenter>image {
  width: 100%;
  height: 100%;
}

.mainBusinessContent>.mainBusinessImage>.mainCenter>text {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  color: #ffffff;
  font-size: 26rpx;
}

.mainBusinessContent>.mainBusinessImage>.mainCenter>.theme {
  font-size: 24rpx;
  color: var(--colorOne);
  position: absolute;
}

.mainBusinessContent>.mainBusinessImage>.mainCenter>.theme>text {
  font-family: DINAlternate, DINAlternate-Bold;
  font-weight: bold;
  color: #0077cb;
  margin-right: 6rpx;
}

.theme1 {
  left: -24rpx;
  top: -50rpx;
}

.theme2 {
  top: -26rpx;
  right: -46rpx;
}

.theme3 {
  top: 52%;
  right: -144rpx;
  transform: translateY(-50%);
}

.theme4 {
  bottom: -44rpx;
  right: -22rpx;
}

.theme5 {
  bottom: -40rpx;
  left: -42rpx;
}

.theme6 {
  top: 49%;
  left: -144rpx;
  transform: translateY(-50%);
}

.corporateCulture {
  margin: 64rpx 0rpx 0 30rpx;
  position: relative;
}

.corporateCulture .allHead {
  margin-right: 30rpx;
}

.certificateList {
  white-space: nowrap;
}

.certificateList>.certificateOne {
  display: inline-block;
  width: 269rpx;
  height: 358rpx;
  padding: 15rpx 18rpx 15rpx 15rpx;
  margin-right: 21rpx;
  background: rgba(251, 251, 251, .8);
}

.certificateList>.certificateOne:last-child {
  margin-right: 0;
}

.certificateList>.certificateOne>image {
  width: 100%;
  height: 100%;
}

.introductionOfExperts {
  margin: 66rpx 0rpx 0 30rpx;
  position: relative;
}

.introductionOfExperts .allHead {
  margin-right: 30rpx;
}

.expertList {
  white-space: nowrap;
}

.expertList>.expertOne {
  display: inline-block;
  width: 270rpx;
  height: 372rpx;
  background: #fbfbfb;
  border-radius: 4rpx;
  padding: 21rpx 23rpx 0;
  margin-right: 21rpx;
  font-size: 24rpx;
  color: #a6abb5;
}

.expertList>.expertOne:last-child {
  margin-right: 0;
}

.expertList>.expertOne>image {
  width: 100%;
  height: 225rpx;
  margin-bottom: 18rpx;
}

.expertList>.expertOne>.expertName {
  color: #000d23;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 14rpx;
}

.brandCustomers {
  margin: 64rpx 30rpx 0 30rpx;
  position: relative;
}

.brandAll {
  margin-top: 40rpx;
}

.brandOne {
  width: 216rpx;
  height: 108rpx;
  border: 2rpx solid #f8f8f8;
  border-radius: 4rpx;
  margin-right: 21rpx;
  margin-bottom: 26rpx;
}
.brandOne>image {
  border-radius: 4rpx;
}

.brandOne:nth-child(3n) {
  margin-right: 0rpx;
}

.brandOne:nth-last-child(-n+3) {
  margin-bottom: 0rpx;
}

.newsCenter {
  margin: 66rpx 0 0;
  position: relative;
}
.newsCenter>.allHead {
  margin: 0 30rpx;

}

.aboutUs {
  margin: 0 30rpx ;
}

.aboutUsDistance {
  margin-bottom: 40rpx;
}

.aboutUsDistance2 {
  margin-left: 6rpx;
}

.aboutContent {
  background: #ffffff;
  border-radius: 4rpx;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0, 13, 35, 0.07);
  padding: 32rpx 24rpx;
  color: var(--colorOne);
  font-weight: bold;
  font-size: 32rpx;
  margin-top: 44rpx;
}

.aboutTitle {
  color: #a6abb5;
  font-size: 24rpx;
  font-weight: normal;
  margin-bottom: 6rpx;
}

.aboutTitleBlue {
  color: #0077cb;
  font-size: 32rpx;
  text-decoration: underline;
  letter-spacing: 1rpx;
}

.aboutIcon {
  width: 40rpx;
  height: 40rpx;
}

.customerService>button>image {
  width: 92rpx;
  height: 92rpx;
}

.customerService>button {
  margin-bottom: 24rpx;
}
.customerService>button:last-child {
  margin-bottom:0rpx;
}
.maodian {
  height: 168rpx;
  width: 100%;
  position: absolute;
  left: 0;
  top: -168rpx;
  background: transparent;
  z-index: -1;
}
.newsList {
  padding:36rpx 30rpx 64rpx
}
.movableArea {
  position: fixed;
  top: 0;
  left: 0;
  pointer-events: none;
  z-index: 5;
  background-color:transparent;
}
.movableView {
  pointer-events: auto;
  width: 100rpx;
  height: auto;
  background-color:transparent;
}